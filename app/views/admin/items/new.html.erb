<h2>商品新規登録</h2>
<%= form_with model: @item,local:true do |f| %>
  <h4>商品画像</h4>
    <%= f.file_field :image, accept: "image/*" %>
  <h4>商品名</h4><%= f.text_field :title %>
  <h4>商品説明</h4><%= f.text_area :body %>
  <h4>税抜価格</h4><%= f.number_field :price %> 円
  <h2>
    <%= f.submit '新規登録' %>
  </h2>
<% end %>