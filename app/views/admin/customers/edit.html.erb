<h4>さんの会員情報詳細</h4>

<%= form_with model: @customer, local:true do |f| %>

<h6>会員ID</h6> <%# ここにオブジェクト.id みたいなもの%>
<h6>氏名</h6>
<%= f.text_field :last_name %><%= f.text_field :first_name %>
<h6>フリガナ</h6>
<%= f.text_field :last_name_kana %><%= f.text_field :first_name_kana %>
<h6>郵便番号</h6>
<%= f.number_field :postal_code %>
<h6>住所</h6>
<%= f.text_field :address %>
<h6>電話番号</h6>
<%= f.telephone_field :phone %>
<h6>メールアドレス</h6>
<%= f.email_field :email %>

<h6>会員ステータス</h6>
<!--以下、後で調べるor聞く-->
<!--%= f.radio_button :status, :male %>-->
<!--%= f.label :status, "有効" %>-->
<!--%= f.radio_button :status, :female %>-->
<!--%= f.label :status, "退会" %>-->

<h4><%= f.submit "変更を保存" %></h4>
<% end %>